<!DOCTYPE html>
<html>
<head>
  <title>BzLabs</title>
  <%= stylesheet_link_tag    "application" %>
  <%= csrf_meta_tags %>
  <%= yield :head %>
  <%= javascript_include_tag 'http://use.typekit.com/odq1cgl.js' %>
  <%= javascript_tag do %>
    try { Typekit.load(); } catch(e) {}
  <% end %>
  <!--[if lt IE 9]>
    <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
</head>
<body>
  <%= yield :before_header %>
  <%= render 'shared/header' %>
  <%= yield :after_header %>

  <div class="content-container">
    <div class="content">
      <%= yield %>
    </div>
  </div>

  <%= yield :before_footer %>
  <%= render 'shared/footer' %>
  <%= yield :after_footer %>

  <div style="display:none;">
    <%= javascript_include_tag "application" %>
    <%= yield :javascripts %>
    <%= render 'shared/ga' %>
  </div>
</body>
</html>

